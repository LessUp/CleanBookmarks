  "adaptive_learning_config": {
    "min_feedback_threshold": 5,
    "confidence_adjustment_factor": 0.15,
    "category_creation_threshold": 3,
    "pattern_strength_decay": 0.95,
    "learning_window_days": 30
  },

  "duplicate_detection_config": {
    "url_similarity_threshold": 0.9,
    "title_similarity_threshold": 0.8,
    "content_hash_enabled": true,
    "domain_grouping_enabled": true,
    "ignore_parameters": ["utm_source", "utm_medium", "utm_campaign", "fbclid", "gclid"]
  }
}