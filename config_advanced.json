{
  "title_cleaning_rules": {
    "prefixes": ["登录 |", "Sign in ·", "登录 ", "Welcome to ", "Home - ", "Index - "],
    "suffixes": [
      "- V2EX", "· GitHub", "| Microsoft Learn", " - 知乎", " - 简书", 
      "| 酷 壳 - CoolShell", " - Stack Overflow", " - Wikipedia", 
      " | Medium", " - Dev.to", " - Hacker News"
    ],
    "replacements": { 
      "&amp;": "&", 
      "&lt;": "<", 
      "&gt;": ">", 
      "--- ": "- ", 
      "(7条消息)": "",
      "【": "[",
      "】": "]"
    }
  },

  "advanced_settings": {
    "classification_threshold": 0.6,
    "semantic_similarity_threshold": 0.7,
    "learning_rate": 0.1,
    "max_categories": 100,
    "content_analysis_enabled": true,
    "adaptive_learning_enabled": true,
    "multi_language_support": true,
    "duplicate_detection_threshold": 0.85,
    "confidence_boost_factor": 1.2,
    "feedback_weight": 0.3
  },

  "processing_order": ["priority_rules", "category_rules", "semantic_rules", "adaptive_rules"],

  "category_order": [
    "工作台",
    "AI & 机器学习",
    "编程 & 开发",
    "生物信息学",
    "技术文档",
    "开源项目",
    "学习资源",
    "工具 & 服务",
    "新闻 & 资讯",
    "社区论坛",
    "娱乐 & 生活",
    "求职 & 职场",
    "稍后阅读",
    "未分类"
  ],

  "semantic_rules": {
    "programming_keywords": {
      "chinese": ["编程", "开发", "代码", "程序", "算法", "数据结构", "软件工程"],
      "english": ["programming", "development", "coding", "algorithm", "software", "engineering"]
    },
    "ai_ml_keywords": {
      "chinese": ["人工智能", "机器学习", "深度学习", "神经网络", "自然语言处理", "计算机视觉"],
      "english": ["artificial intelligence", "machine learning", "deep learning", "neural network", "nlp", "computer vision"]
    },
    "bioinformatics_keywords": {
      "chinese": ["生物信息", "基因组", "蛋白质", "序列分析", "生物统计"],
      "english": ["bioinformatics", "genomics", "proteomics", "sequence analysis", "biostatistics"]
    }
  },

  "domain_intelligence": {
    "AI & 机器学习": {
      "primary_domains": ["openai.com", "huggingface.co", "deepmind.com", "anthropic.com", "cohere.com"],
      "secondary_domains": ["arxiv.org", "paperswithcode.com", "towards.ai", "medium.com"],
      "path_patterns": ["/ai/", "/ml/", "/neural/", "/deep-learning/"],
      "title_patterns": ["GPT", "ChatGPT", "BERT", "Transformer", "LLM", "大模型"]
    },
    "编程 & 开发": {
      "primary_domains": ["github.com", "gitlab.com", "stackoverflow.com", "developer.mozilla.org"],
      "secondary_domains": ["dev.to", "codepen.io", "replit.com", "codesandbox.io"],
      "path_patterns": ["/docs/", "/api/", "/tutorial/", "/guide/"],
      "title_patterns": ["API", "Documentation", "Tutorial", "Guide", "开发文档"]
    },
    "生物信息学": {
      "primary_domains": ["ncbi.nlm.nih.gov", "ensembl.org", "uniprot.org", "bioconductor.org"],
      "secondary_domains": ["nature.com", "science.org", "plos.org", "biorxiv.org"],
      "path_patterns": ["/bio/", "/genomics/", "/proteomics/", "/bioinformatics/"],
      "title_patterns": ["Genome", "Protein", "Sequence", "BLAST", "基因", "蛋白质"]
    }
  },

  "priority_rules": {
    "工作台/公司内部": {
      "weight": 100,
      "rules": [
        { 
          "match": "domain", 
          "keywords": ["zego.im", "zego.cloud", "bgi.com", "genomics.cn", "dingtalk.com", "feishu.cn"],
          "weight": 100
        },
        {
          "match": "title",
          "keywords": ["内部", "公司", "工作", "项目", "团队"],
          "weight": 80
        }
      ]
    }
  },

  "category_rules": {
    "AI & 机器学习/大语言模型": {
      "rules": [
        { 
          "match": "domain", 
          "keywords": ["openai.com", "anthropic.com", "cohere.com", "huggingface.co", "deepseek.com"], 
          "weight": 20 
        },
        { 
          "match": "title", 
          "keywords": ["GPT", "ChatGPT", "Claude", "LLM", "大语言模型", "Transformer"], 
          "weight": 15 
        },
        {
          "match": "path",
          "keywords": ["gpt", "llm", "language-model", "chatbot"],
          "weight": 12
        }
      ]
    },
