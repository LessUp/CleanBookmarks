{
  "show_confidence_indicator": false,
  "ai_settings": {
    "confidence_threshold": 0.7,
    "use_semantic_analysis": true,
    "use_user_profiling": true,
    "cache_size": 10000,
    "max_workers": 4,
    "enable_learning": true
  },
  "llm": {
    "enable": false,
    "provider": "openai",
    "base_url": "https://api.openai.com",
    "model": "gpt-4o-mini",
    "api_key_env": "OPENAI_API_KEY",
    "temperature": 0.0,
    "top_p": 1.0,
    "timeout_seconds": 25,
    "max_retries": 1
  },
  "title_cleaning_rules": {
    "prefixes": [
      "登录 |",
      "Sign in ·",
      "登录 "
    ],
    "suffixes": [
      "- V2EX",
      "· GitHub",
      "| Microsoft Learn",
      " - 知乎",
      " - 简书",
      " | 酷 壳 - CoolShell"
    ],
    "replacements": {
      "&": "&",
      "<": "<",
      ">": ">",
      "--- ": "- ",
      "(7条消息)": ""
    }
  },
  "processing_order": [
    "priority_rules",
    "category_rules"
  ],
