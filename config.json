{
  "processing_order": [
    "special_domain_rules",
    "format_rules",
    "read_later_rules",
    "category_rules"
  ],
  "special_domain_rules": {
    "技术栈/代码 & 开源/GitHub": {
      "domains": ["github.com"],
      "split_by_path_segment": 1
    },
    "技术栈/云服务 & DevOps": {
      "domains": ["docker.com", "kubernetes.io", "helm.sh", "circleci.com", "travis-ci.org"]
    }
  },
  "format_rules": {
    "媒体库/视频": {
      "domains": [
        "youtube.com", "youtu.be",
        "vimeo.com",
        "bilibili.com",
        "ted.com"
      ],
      "url_contains": []
    },
    "媒体库/音乐与播客": {
      "domains": [
        "spotify.com",
        "soundcloud.com",
        "music.apple.com",
        "music.163.com"
      ],
      "url_contains": []
    },
    "资料库/幻灯片": {
      "domains": [
        "slides.com",
        "speakerdeck.com",
        "slideshare.net"
      ],
      "url_contains": [
        "docs.google.com/presentation"
      ]
    },
    "资料库/文档": {
      "domains": [],
      "url_contains": [],
      "url_ends_with": [
        ".pdf", ".epub", ".mobi",
        ".docx", ".pptx", ".doc", ".xls", ".xlsx"
      ]
    },
    "效率工具/在线应用": {
      "domains": [
        "figma.com", "framer.com",
        "canva.com",
        "miro.com",
        "excalidraw.com",
        "regex101.com",
        "codesandbox.io", "codepen.io", "replit.com",
        "tinypng.com", "tinyjpg.com",
        "tldraw.com"
      ],
      "url_contains": []
    }
  },
  "read_later_rules": {
    "domains": [
      "medium.com", "dev.to", "freecodecamp.org", "hashnode.com",
      "hackernoon.com", "dzone.com", "infoq.com", "juejin.cn", "segmentfault.com",
      "stackoverflow.com", "stackexchange.com", "martinfowler.com", "css-tricks.com", 
      "smashingmagazine.com", "thenewstack.io", "techcrunch.com", "stratechery.com", 
      "news.ycombinator.com", "baeldung.com", "geeksforgeeks.org", "tutorialspoint.com", 
      "w3schools.com", "towardsdatascience.com", "zhihu.com", "yuque.com", "jianshu.com"
    ],
    "keywords_in_title": [
      "blog", "article", "post", "tutorial", "guide", "cheatsheet", "deep-dive",
      "文章", "教程", "指南", "博客", "分享", "解析", "实践", "入门"
    ],
    "path_restrictions": {
      "zhihu.com": ["/p/", "/column/"],
      "yuque.com": ["/docs/", "/posts/"]
    }
  },
  "category_rules": {
    "技术栈/云服务 & DevOps": {
      "keywords": [
        "aws", "amazon", "google cloud", "gcp", "azure", "microsoft", 
        "docker", "kubernetes", "helm", "terraform", "ansible",
        "vercel", "netlify", "cloudflare", "heroku"
      ]
    },
    "技术栈/Web 开发": {
      "keywords": [
        "react", "vue", "angular", "svelte", "next.js", "nuxt.js",
        "webpack", "vite", "css", "html", "javascript", "typescript",
        "nodejs", "deno", "express"
      ]
    },
    "技术栈/后端 & 数据库": {
      "keywords": [
        "python", "django", "flask", "fastapi",
        "rust", "golang", "go",
        "java", "spring",
        "sql", "mysql", "postgresql", "sqlite", "mongodb", "redis"
      ]
    },
    "技术栈/AI & 数据科学": {
      "keywords": [
        "tensorflow", "pytorch", "keras", "scikit-learn", "pandas", "numpy",
        "jupyter", "colab", "kaggle",
        "openai", "claude", "gemini", "huggingface", "llama", "mistral"
      ]
    },
    "技术栈/综合": {
      "keywords": [
        "stackoverflow.com",
        "github.com",
        "dev.to",
        "medium.com",
        "juejin.cn",
        "segmentfault.com"
      ]
    },
    "设计 & 创意": {
      "keywords": [
        "dribbble.com",
        "behance.net",
        "pinterest.com",
        "design"
      ]
    },
    "新闻 & 资讯": {
      "keywords": [
        "news.ycombinator.com",
        "techcrunch.com",
        "theverge.com",
        "reuters.com",
        "bloomberg.com",
        "wsj.com"
      ]
    }
  },
  "domain_based_subcats": []
} 